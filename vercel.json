{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "version": 2,
  "builds": [
      {
          "src": "api/server.js",
          "use": "@vercel/node",
          "config": {
              "includeFiles": [
                  "db.json"
              ]
          }
      }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}